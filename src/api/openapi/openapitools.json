{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.1",
    "generators": {
      "KRAVKOMPASSET-UI CLIENT API GENERATOR": {
        "generatorName": "typescript-angular",
        "output": "../src/lib",
        "glob": "swagger-api.yml",
        "additionalProperties": {
          "ngVersion": "13.3.11",
          "supportsES6": true,
          "providedInRoot": false,
          "withInterfaces": true,
          "stringEnums": true,
          "configurationClassName": "Configuration",
          "configurationParametersInterfaceName": "ConfigurationInterfaceName",
          "serviceFileSuffix": ".service",
          "fileNaming": "kebab-case"
        }
      }
    }
  }
}
